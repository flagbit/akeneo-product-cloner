{
  "name": "flagbit/product-cloner-bundle",
  "description": "The Flagbit Product Cloner Bundle gives you the possibility to quick cloning products within Akeneo PIM",
  "keywords": ["akeneo", "product", "cloner"],
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Antonio Mansilla",
      "email": "antonio.mansilla@flagbit.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Flagbit\\Bundle\\ProductClonerBundle\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/akeneo/pim-community-dev.git",
      "branch": "master",
      "no-api": true
    }
  ],
  "require": {
    "akeneo/pim-community-dev": "^2.0"
  },
  "require-dev": {
    "phpspec/phpspec": "*",
    "squizlabs/php_codesniffer": "*",
    "overtrue/phplint": "*"
  }
}
